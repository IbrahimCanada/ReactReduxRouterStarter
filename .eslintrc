{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  // "extends": ["standard", "react-app", "airbnb",  "eslint:recommended"],
  "extends": ["react-app", "airbnb", "standard"],
  "parser": "babel-eslint",
  "rules": {
    // "quotes": [2, "single"],
    // "strict": [2, "never"],
    // "react/jsx-uses-react": 2,
    // "react/jsx-uses-vars": 2,
    // "react/react-in-jsx-scope": 2,
    "no-console": 0,
    "react/jsx-filename-extension": [2, { "extensions": [".js"] }]
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ]
}
