{
  "name": "reactreduxexpressstarterapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/app.js",
    "dev": "nodemon ./src/bin/www --exec \"node -r dotenv/config -r babel-core/register\"",
    "clean": "rimraf dist",
