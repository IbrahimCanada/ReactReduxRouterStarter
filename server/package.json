    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "express-session": "^1.15.5",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "helmet": "^3.8.1",
    "http-status": "^1.0.1",
    "ignore-styles": "^5.0.1",
    "joi": "^10.6.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.5",
    "multer": "^1.3.0",
    "pug": "~2.0.0-beta11",
